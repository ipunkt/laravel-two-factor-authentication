{
	"name": "ipunkt/laravel-two-factor-authentication",
	"description": "Two factor authentication (2FA) with TOTP tokens. (RFC 6238)",
	"keywords": [
		"laravel",
		"two-factor",
		"authentication",
		"2fa",
		"rfc-6238"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Robert Kummer",
			"email": "rok@ipunkt.biz"
		}
	],
	"require": {
		"php": ">=5.6.4",
		"pragmarx/google2fa": "~1.0",
		"paragonie/constant_time_encoding": "~2.0",
		"bacon/bacon-qr-code": "~1.0",
		"ipunkt/laravel-package-manager": "^0.2.0"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
		"phpunit/phpunit": "~5.0"
	},
	"autoload": {
		"psr-4": {
			"Ipunkt\\Laravel\\TwoFactorAuthentication\\": "src/"
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
